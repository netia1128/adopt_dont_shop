<h1>All pets</h1>

<%= button_to "Start an Adoption Application", "/applications/new", method: :get %><br>
<%= render 'pets_view', pets: @pets %>

<%= form_with url: "/pets", method: :get, local: true do |f| %>
  <%= f.label "Search Pets By Name" %>
  <%= f.text_field :search %>
  <%= f.submit "Search" %>
<% end %><br>

